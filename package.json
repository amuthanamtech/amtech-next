    "formidable": "^3.5.4",
    "next": "^15.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-phone-input-2": "^2.15.1",
    "resend": "^4.0.1-alpha.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.13",
    "@cloudflare/workers-types": "^4.20250724.0",
