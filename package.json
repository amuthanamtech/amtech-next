{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env-shell next dev",
    "dev:debug": "set NODE_OPTIONS=--inspect npm run dev",
    "build": "cross-env-shell next build",
